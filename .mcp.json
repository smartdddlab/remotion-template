{
  "mcpServers": {
    "remotion-documentation": {
      "command": "npx",
      "args": ["@remotion/mcp@latest"]
    },
    "minimax-mcp": {
      "command": "npx",
      "args": ["-y", "minimax-mcp-js"],
      "env": {
        "MINIMAX_API_HOST": "https://api.minimaxi.com",
        "MINIMAX_API_KEY": "",
        "MINIMAX_MCP_BASE_PATH": "./public",
        "MINIMAX_RESOURCE_MODE": "local"
      },
      "transport": "studio"
    }
  }
}
